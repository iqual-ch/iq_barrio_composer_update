{
  "name": "iqual/iq_barrio_composer_update",
  "description": "Backup iq_barrios _definitions.scss file",
  "type": "composer-plugin",
  "repositories": [
    {
      "type": "composer",
      "url": "https://github.com/iqual-ch/iq_barrio_composer_update"
    }
  ],
  "authors": [
    {
      "name": "Philippe von Bergen",
      "email": "ph.vonbergen@iqual.ch"
    }
  ],
  "require": {
    "php": "^8.1",
    "composer-plugin-api": "^2.0",
    "drupal/core-composer-scaffold": "^9.5 || ^10"
  },
  "extra": {
    "class": "iqual\\Composer\\Plugin\\UpdatePlugin"
  },
  "autoload": {
    "psr-4": {
      "iqual\\Composer\\": "src"
    }
  }
}
